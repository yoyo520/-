.circleProgress_wrapper{
        width:97rpx;
        height:97rpx;
        margin:12rpx auto;
        position: relative;
        }
        .wrapper{
           width:49rpx;
           height:100rpx;
            position: absolute;
            top:5rpx;
            overflow: hidden;
        }
        .right{
            right:0;
        }
        .left{
            left:0;
        }
        .circleProgress{
            width:70rpx;
            height:70rpx;
            border:15rpx solid white;
            border-radius: 50%;
            position: absolute;
            top:0;
           transform: rotate(45deg);
        }
        .rightcircle{
            border-top:15rpx solid #cc334e;
            border-right:15rpx solid #cc334e;
            right:0;
            animation: circleProgressLoad_right 10s linear infinite;
        }
        .leftcircle{
            border-bottom:15rpx solid #cc334e;
            border-left:15rpx solid #cc334e;
            left:0;
            animation: circleProgressLoad_left 10s linear infinite;
        }
        @keyframes circleProgressLoad_right{
            0%{
                -webkit-transform: rotate(45deg);
            }
            50%{
                -webkit-transform: rotate(225deg);
            }
            100%{
                -webkit-transform: rotate(225deg);
            }
        }
        @keyframes circleProgressLoad_left{
            0%{
                -webkit-transform: rotate(45deg);
            }
            50%{
                -webkit-transform: rotate(45deg);
            }
            100%{
                -webkit-transform: rotate(225deg);
            }
        }
